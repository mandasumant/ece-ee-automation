        }

        stage('Prepare environment') {
            when {
                not {
                    anyOf {
                        triggeredBy 'TimerTrigger'
                        expression {
                            params.CJT == true ||
                                    params.APOLLO_Q2O == true ||
                                    params.APOLLO_FLEX == true ||
                                    params.APOLLO_FLEX_MOE == true ||
