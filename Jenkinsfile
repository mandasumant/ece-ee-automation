                                    params.APOLLO_PAY_INVOICE == true
                        }
                    }
                }
            }
            steps {
                script {
                    common_sonar = new ors.utils.CommonSonar(steps, env, docker)

                    // pull source
                    checkout scm
                }
            }
        }

        stage('Build Maven Project') {
            when {
                not {
                    anyOf {
                        triggeredBy 'TimerTrigger'
                        expression {
                            params.CJT == true ||
                                    params.APOLLO_Q2O == true ||
                                    params.APOLLO_FLEX == true ||
                                    params.APOLLO_FLEX_MOE == true ||
                                    params.APOLLO_R2_0_4 == true ||
                                    params.EDU == true ||
                                    params.APOLLO_TTR == true ||
                                    params.APOLLO_PAY_INVOICE == true
                        }
                    }
                }
            }
            agent {
                docker {
                    image 'artifactory.dev.adskengineer.net/autodeskcloud/ctr-ci-slave-jdk-11:latest'
                    args '-v /mnt/data/.m2:/home/jenkins/.m2'
                    reuseNode true
                }
            }
            steps {
                script {
                    sh "mvn clean install -DskipTests"
                }
            }
        }

        stage('SonarQube code quality scan') {
            when {
                not {
                    anyOf {
                        triggeredBy 'TimerTrigger'
                        expression {
                            params.CJT == true ||
                                    params.APOLLO_Q2O == true ||
                                    params.APOLLO_FLEX == true ||
                                    params.APOLLO_FLEX_MOE == true ||
                                    params.APOLLO_R2_0_4 == true ||
                                    params.APOLLO_TTR == true ||
                                    params.APOLLO_PAY_INVOICE == true ||
                                    params.EDU == true
                        }
                    }
                }
            }
            agent {
                docker {
                    image 'artifactory.dev.adskengineer.net/autodeskcloud/ctr-ci-slave-jdk-11:latest'
                    args '-v /mnt/data/.m2:/home/jenkins/.m2'
                    reuseNode true
                }
            }
            steps {
                script {
                    common_sonar.do_maven_scan(trunk: "${env.BRANCH_NAME}")
                }
            }
        }
        stage('CJT Nightly Regression') {
            when {
                branch 'master'
                triggeredBy 'TimerTrigger'
            }
            steps {
                triggerCJT(serviceBuildHelper, 'INT')
                script {
                    echo 'Sleeping 1 min. between next CJT'
                    sh 'sleep 60'
                }
                triggerCJT(serviceBuildHelper, 'STG')
            }
        }
        stage('CJT Regression') {
            when {
                branch 'master'
                expression {
                    params.CJT == true
                }
            }
            steps {
                triggerCJT(serviceBuildHelper, params.ENVIRONMENT)
            }
        }
        stage('Nightly sleep') {
            when {
                branch 'master'
                triggeredBy 'TimerTrigger'
            }
            steps {
                echo 'Sleeping 10 min.'
                script {
                    sh 'sleep 600'
                }
            }
        }
        stage('Apollo Quote 2 Order - TTR') {
            when {
                branch 'master'
                expression {
                    params.APOLLO_TTR == true
                }
            }
            steps {
                triggerApolloTTR(serviceBuildHelper, params.ENVIRONMENT)
                script {
                    sh 'sleep 600'
                }
            }
        }
        stage('Apollo Quote 2 Order - TTR - Regression') {
            when {
                branch 'master'
                triggeredBy 'TimerTrigger'
            }
            steps {
                triggerApolloTTR(serviceBuildHelper, 'INT')
                script {
                    sh 'sleep 600'
                }
            }
        }

        stage('Apollo R2.0.4') {
            when {
                branch 'master'
                anyOf {
                    triggeredBy 'TimerTrigger'
                    expression {
                        params.APOLLO_R2_0_4 == true
                    }
                }
            }
            steps {
                triggerApolloR2_0_4(serviceBuildHelper, 'INT')
                script {
                    sh 'sleep 600'
                }
            }
        }
        stage('Apollo Quote 2 Order - Pay Invoice') {
            when {
                branch 'master'
                expression {
                    params.APOLLO_PAY_INVOICE == true
                }
            }
            steps {
                triggerApolloR2_3PayInvoice(serviceBuildHelper)
            }
        }
        stage('Apollo Quote 2 Order') {
            when {
                branch 'master'
                anyOf {
                    triggeredBy 'TimerTrigger'
                    expression {
                        params.APOLLO_Q2O == true
                    }
                }
            }
            steps {
                triggerApolloR2_3(serviceBuildHelper)
                script {
                    sh 'sleep 600'
                }
            }
        }
        stage('Apollo Flex Direct Order Suite') {
            when {
                branch 'master'
                anyOf {
                    triggeredBy 'TimerTrigger'
                    expression {
                        params.APOLLO_FLEX == true
                    }
                }
            }
            steps {
                triggerApolloDirectFlexR2_2(serviceBuildHelper)
                script {
                    sh 'sleep 60'
                }
            }
        }

